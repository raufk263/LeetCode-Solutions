Approach:-Find the sum till n and subtract the sum of given array.

int missingNumber(vector<int>& nums) {
        int len=nums.size();
        int value=(len)*(len+1)/2;
        int sum=0;
        for(int i=0; i<nums.size(); i++){
            sum+=nums[i];
        }
        return value-sum;
    }
